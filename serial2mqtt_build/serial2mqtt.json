{
    "mqtt": {
        "connection": "tcp://localhost:1883"
    },
    "serial": {
        "baudrate": 115200,
        "ports": [
            "/dev/ttyUSB0"
        ],
        "protocol": "jsonObject",
		"idleTimeout": 20000
    },
    "log": {
        "protocol": true,
        "debug": true,
        "useColors": true,
        "mqtt": false,
        "program": false,
        "level": "I",
        "file": "log.serial2mqtt.",
        "console": false
    }
}
